<launch>

  <!-- INFORMATION -->
  <!-- static_transform_publisher args: x y z yaw pitch roll frame_id child_frame_id period_in_ms -->

  <!-- launch avc_sensors publisher nodes and broadcast transforms -->
  <include file="$(find avc_sensors)/launch/sensors.launch" />

  <!-- launch GPS publisher node and broadcast transform -->
  <include file="$(find nmea_navsat_driver)/launch/nmea_navsat_driver.launch" />
  <node name="gps_to_base_tf" pkg="tf" type="static_transform_publisher" args="0 0 0 0 0 0 base_link gps_link 10" />

</launch>
